                                           }
                                           else
                                           {
                                               <option value="education" id="edu"> Education </option>
                                           } }
                                       @{if (info.firstLegal == "juvenilejustice")
                                           {
                                               <option value="juvenilejustice" id="juv" selected>Juvenile Justice</option>
                                           }
                                           else
                                           {
                                               <option value="juvenilejustice" id="juv">Juvenile Justice</option>
                                           } }
                                       @{if (info.firstLegal == "dependency")
                                           {
                                               <option value="dependency" id="dependency" selected>Dependency</option>
                                           }
                                           else
                                           {
                                               <option value="dependency" id="dependency">Dependency</option>
                                           } }
                                       @{if (info.firstLegal == "bakeract")
                                           {
                                               <option value="bakeract" id="edu" selected>Baker Act</option>
                                           }
                                           else
                                           {
                                               <option value="bakeract" id="edu">Baker Act</option>
                                           } }
                                       @{if (info.firstLegal == "familylaw")
                                           {
                                               <option value="familylaw" id="famlaw" selected>Family Law</option>
                                           }
                                           else
                                           {
                                               <option value="familylaw" id="famlaw">Family Law</option>
                                           } }
                                       @{if (info.firstLegal == "immigration")
                                           {
                                                                              <option value="immigration" id="immig" selected>Immigration</option>
                                           }
                                           else
                                           {
                                   <option value="immigration" id="immig">Immigration</option>
                                           } }
                                       @{if (info.firstLegal == "publicbenefits")
                                           {
                                                                              <option value="publicbenefits" id="publicben" selected>Public Benefits</option>
                                           }
                                           else
                                           {
                                   <option value="publicbenefits" id="publicben">Public Benefits</option>
                                           } }
                                       @{if (info.firstLegal == "housing")
                                           {
                                                                              <option value="housing" id="housing" selected>Housing</option>
                                           }
                                           else
                                           {
                                   <option value="housing" id="housing">Housing</option>
                                           } }
                                       @{if (info.firstLegal == "other")

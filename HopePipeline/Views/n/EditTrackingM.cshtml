    <div id="page4">
        <h2> Center for Children's Rights information</h2>
        <br />
        <ul>
            <li>

                Referral Date and Time <br />
                <input asp-for="referralDate" type="datetime-local" class="field-style field-full align-none" id="refdate"
                       value="@info.referralDate"
                       min="2000-06-07T00:00" max="2060-06-14T00:00">
                <br />
            </li>

            <li>
                School at time of Case Closure <br />
                <input asp-for="schoolAtClosure" type="text" value="@info.schoolAtClosure" class="field-style field-full align-none" id="school" /> <br />
            </li>

            <li>
                Email of First Referral Source<br />
                <input asp-for="emailOfFirstReferralSource" type="text" value="@info.emailOfFirstReferralSource" class="field-style field-full align-none" id="referralemail" /> <br />
            </li>

            <li>
                Level of Service Provided <br />
                @{if (info.levelOfServiceProvided == "none")
                    { <input asp-for="levelOfServiceProvided" type="radio" id="none" value="n" checked /> }
                    else
                    { <input asp-for="levelOfServiceProvided" type="radio" id="none" value="n" />} }
                None<br />
                @{if (info.levelOfServiceProvided == "infoEdu")
                    { <input asp-for="levelOfServiceProvided" type="radio" id="infoEdu" value="info" checked />}
                    else
                    {<input asp-for="levelOfServiceProvided" type="radio" id="infoEdu" value="info" /> }}
                information/Education<br />
                @{if (info.levelOfServiceProvided == "service")
                    {<input asp-for="levelOfServiceProvided" type="radio" id="serviceAdvice" value="service" checked> }
                    else
                    {<input asp-for="levelOfServiceProvided" type="radio" id="serviceAdvice" value="service"> }} Brief Service/Advice<br />
                @{if (info.levelOfServiceProvided == "full")
                    { <input asp-for="levelOfServiceProvided" type="radio" id="full" value="full" checked /> }
                    else
                    { <input asp-for="levelOfServiceProvided" type="radio" id="full" value="full" /> } }
                Full<br />
                
            </li>

            <li>
                Reason for Nonengagement <br />
                @{if (info.nonEngagementReason == "n")
                    { <input asp-for="nonEngagementReason" type="radio" id="contact" value="n" checked>}
                    else
                    {<input asp-for="nonEngagementReason" type="radio" id="contact" value="n"> }
                }Unable to Contact<br />
                @{if (info.nonEngagementReason == "info")
                    {<input asp-for="nonEngagementReason" type="radio" id="intake" value="info" checked> }
                    else
                    {<input asp-for="nonEngagementReason" type="radio" id="intake" value="info"> }
                }
                Unable to Complete Intake<br />
                @{if (info.nonEngagementReason == "service")
                    {<input asp-for="nonEngagementReason" type="radio" id="declined" value="service" checked>}
                    else
                    {<input asp-for="nonEngagementReason" type="radio" id="declined" value="service">}
                } Family Declined Services<br />
               
            </li>

            <li>
                Case Status <br />
                @{if (info.caseStatus == 1)
                    { <input asp-for="caseStatus" type="radio" id="openclose" value="1" checked> }
                    else
                    {<input asp-for="caseStatus" type="radio" id="openclose" value="1"> }}
                Open<br />
                @{if (info.caseStatus == 0)
                    { <input asp-for="caseStatus" type="radio" id="close" value="0" checked> }
                    else
                    {<input asp-for="caseStatus" type="radio" id="close" value="0" /> }}
                Closed<br />
                @{if (info.caseStatus == 2)
                    {<input asp-for="caseStatus" type="radio" id="nonEngageClose" value="2" checked> }
                    else
                    {<input asp-for="caseStatus" type="radio" id="nonEngageClose" value="2"> }}
                Closed due to nonengagement<br />
            </li>

            <li>
                Legal Advocacy <br />
                @{if (info.legalAdvocacy == 1)
                    { <input asp-for="legalAdvocacy" type="radio" id="yes" value="1" checked> }
                    else
                    { <input asp-for="legalAdvocacy" type="radio" id="yes" value="1"> } }
                Yes <br />
                @{if (info.legalAdvocacy == 0)
                    { <input asp-for="legalAdvocacy" type="radio" id="no" value="0" checked> }
                    else
                    { <input asp-for="legalAdvocacy" type="radio" id="no" value="0"> } }
                No <br />

            </li>
            <li>
            Legal Advocacy Taken
            <br /><input asp-for="legalAdvoTaken" type="text" value="@info.legalAdvoTaken" class="field-style field-split align-left" placeholder="legalAdvoTaken" />
            </li>

            <li>
               <br /> Remedy Resolution <br />
                @{if (info.remedyResolution == 1)
                    { <input asp-for="remedyResolution" type="radio" id="yes" value="1" checked> }
                    else
                    { <input asp-for="remedyResolution" type="radio" id="yes" value="1"> } }
                Yes <br />
                @{if (info.remedyResolution == 0)
                    { <input asp-for="remedyResolution" type="radio" id="no" value="0" checked> }
                    else
                    { <input asp-for="remedyResolution" type="radio" id="no" value="0"> } }
                No <br />


            </li>

            <li>
                Re-arrest while Being Represented <br />

                @{if (info.rearrestWhileRepresented == 1)
                    { <input asp-for="rearrestWhileRepresented" type="radio" name="rearr" id="yes" value="1" checked> }
                    else
                    { <input asp-for="rearrestWhileRepresented" type="radio" name="rearr" id="yes" value="1"> } }
                Yes <br />
                @{if (info.rearrestWhileRepresented == 0)
                    { <input asp-for="rearrestWhileRepresented" type="radio" name="rearr" id="no" value="0" checked> }
                    else
                    { <input asp-for="rearrestWhileRepresented" type="radio" name="rearr" id="no" value="0"> } }
                No <br />

            </li>

            <li>
                Advocacy at Court? <br />
                @{if (info.courtAdvocacy == 1)
                    { <input asp-for="courtAdvocacy" type="radio" id="yes" value="1" checked> }
                    else
                    { <input asp-for="courtAdvocacy" type="radio" id="yes" value="1"> } }
                Yes <br />
                @{if (info.courtAdvocacy == 0)
                    { <input asp-for="courtAdvocacy" type="radio" id="no" value="0" checked> }
                    else
                    { <input asp-for="courtAdvocacy" type="radio" id="no" value="0"> } }
                No <br />
            </li>

            <li>
                Advocacy at Staffing?<br />
                @{if (info.staffAdvocacy == 1)
                    { <input asp-for="staffAdvocacy" type="radio" id="yes" value="1" checked> }
                    else
                    { <input asp-for="staffAdvocacy" type="radio" id="yes" value="1"> }
                }Yes<br />
                @{if (info.staffAdvocacy == 0)
                    { <input asp-for="staffAdvocacy" type="radio" id="no" value="0" checked> }
                    else
                    { <input asp-for="staffAdvocacy" type="radio" id="no" value="0"> }
                } No<br />
            </li>

            <li>
                Rearrest advocacy?<br />
                @{if (info.rearrestAdvocacy == 1)
                    { <input asp-for="rearrestAdvocacy" type="radio" id="yes" value="1" checked> }
                    else
                    { <input asp-for="rearrestAdvocacy" type="radio" id="yes" value="1"> }
                }Yes<br />
                @{if (info.rearrestAdvocacy == 0)
                    { <input asp-for="rearrestAdvocacy" type="radio" id="no" value="0" checked> }
                    else
                    { <input asp-for="rearrestAdvocacy" type="radio" id="no" value="0"> }
                }
                No<br />
            </li>

        </ul>



        <button type="submit" class="btn btn-primary">Submit</button>
        <br />
        <br />
    </div>


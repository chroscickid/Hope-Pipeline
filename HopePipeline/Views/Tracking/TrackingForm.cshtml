                <input asp-for="levelOfServiceProvided" type="radio" name="lvlServices" id="serviceAdvice" value="service"> Brief Service/Advice<br />
                <input asp-for="levelOfServiceProvided" type="radio" name="lvlServices" id="full" value="full" /> Full<br />
                <br />
            </li>

            <li>
                Reason for Nonengagement <br />
                <input asp-for="nonEngagementReason" type="radio" name="noneng" id="contact" value="n"> Unable to Contact<br />
                <input asp-for="nonEngagementReason" type="radio" name="noneng" id="intake" value="info"> Unable to Complete Intake<br />
                <input asp-for="nonEngagementReason" type="radio" name="noneng" id="declined" value="service"> Family Declined Services<br />
                <br />
            </li>

            <li>
                Case Status <br />
                <input asp-for="caseStatus" type="radio" name="openclose" id="openclose" value="o"> Open<br />
                <input asp-for="caseStatus" type="radio" name="openclose" id="close" value="c" /> Closed<br />
                <input asp-for="caseStatus" type="radio" name="openclose" id="nonEngageClose" value="nec"> Closed due to nonengagement<br />
            </li>

            <li>
                Legal Advocacy Taken <br />
                <input asp-for="legalAdvocacy" type="radio" name="legaladv" id="yes" value="yes"> Yes<br />
                <input asp-for="legalAdvocacy" type="radio" name="legaladv" id="no" value="no" /> No<br />
            </li>

            <li>
                Remedy Resolution <br />
                <input asp-for="remedyResolution" type="radio" name="remres" id="yes" value="yes"> Yes<br />
                <input asp-for="remedyResolution" type="radio" name="remres" id="no" value="no" /> No<br />
            </li>

            <li>
                Re-arrest while Being Represented <br />
                <input asp-for="rearrestWhileRepresented" type="radio" name="rearr" id="yes" value="yes"> Yes<br />
                <input asp-for="rearrestWhileRepresented" type="radio" name="rearr" id="no" value="no" /> No<br />
            </li>

            <li>
                <label> Is this the first referral? </label>
                <br />
                Yes
                <input asp-for="firstReferral" type="radio" name="ref" id="yesRef" value="yes">
                No
                <input asp-for="firstReferral" type="radio" name="ref" id="noRef" value="no" />
                <br />
                <br />

                <div id="ifNoRef" class="field-style field-full align-none" />
                How many times have they been referred?
                <br />
                <select asp-for="referralCount" name="suspended">
                    <option value="2">2</option>
                    <option value="3">3</option>
                    <option value="4+">4+</option>
                </select>
                <br />

                <div id="ifYesRef">

                </div>
            </li>

            <li>
                Advocacy at Court? <br />
                <input asp-for="courtAdvocacy" type="radio" name="advCourt" id="yes" value="yes"> Yes <br />
                <input asp-for="courtAdvocacy" type="radio" name="advCourt" id="no" value="no" /> No <br />
            </li>

            <li>
                Advocacy at Staffing?<br />
                <input asp-for="staffAdvocacy" type="radio" name="advStaffing" id="yes" value="yes"> Yes<br />
                <input asp-for="staffAdvocacy" type="radio" name="advStaffing" id="no" value="no" /> No<br />
            </li>

        </ul>



        <button type="submit" class="btn btn-primary">Submit</button>
        <br />
